{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/layout.jsx", ".next/types/app/users/home2/page.tsx", ".next/types/app/users/page.tsx", ".next/types/app/users/[id]/page.tsx", "src/app/(auth)/layout.jsx", "src/app/parallroute/@Route2/page.jsx", "src/app/dashbord/@notification/page.jsx", "src/app/dashbord/@revenue/page.jsx", "src/app/dashbord/@users/page.jsx", "src/app/products/[id]/error.jsx"],
  "exclude": ["node_modules"]
}
